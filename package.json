{
  "name": "widget-sdk",
  "version": "1.0.0",
  "description": "Pixelbin.io web widget sdk",
  "main": "dist/widget-sdk.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "serve": "http-server -p 9000 -c-1 -o examples/index.html",
    "serve:iframe": "http-server -p 9001 -c-1 examples/mock-iframe-app",
    "dev": "run-p watch serve serve:iframe",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ankit Gupta",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
